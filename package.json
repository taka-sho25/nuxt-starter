{
  "name": "nuxt-app",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "cross-env NODE_ENV=development APP_STAGE=development nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "start": "nuxi start",
    "lint": "eslint .",
    "fix": "eslint --fix .",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
    "prettier": "prettier --write '**/*.{js,ts,vue,json,css}'",
    "test": "vitest",
    "test:update": "vitest -u",
    "e2e": "npx playwright test",
    "playwright:install": "npx playwright install --with-deps",
    "playwright:ci": "SHARD=\"$((${CIRCLE_NODE_INDEX}+1))\"; npx playwright test --shard=${SHARD}/${CIRCLE_NODE_TOTAL}",
    "playwright": "npx playwright test",
    "storybook": "cross-env APP_STAGE=development storybook dev --port 6006",
    "api:build": "aspida && aspida-mock",
    "prism": "prism mock ./__mock__/mock.yaml -p 3507 -h localhost",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint .",
      "prettier --write '**/*.{js,ts}'"
    ],
    "*.vue": [
      "eslint .",
      "stylelint **/*.vue",
      "prettier --write **/*.vue"
    ],
    "*.css": "stylelint **/*.css"
  },
  "dependencies": {
    "@aspida/fetch": "^1.14.0",
    "@nuxt/eslint": "^1.3.0",
    "@nuxt/icon": "^1.12.0",
    "@vee-validate/zod": "^4.15.0",
    "aspida": "^1.14.0",
    "aspida-mock": "^0.11.1",
    "dayjs": "^1.11.13",
    "nuxt": "^3.17.0",
    "typescript": "^5.8.3",
    "vee-validate": "^4.13.1",
    "vue-router": "^4.5.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^1.0.0",
    "@nuxt/image": "^1.10.0",
    "@nuxt/test-utils": "^3.17.2",
    "@nuxtjs/google-fonts": "^3.2.0",
    "@nuxtjs/storybook": "^8.3.3",
    "@playwright/test": "^1.52.0",
    "@stoplight/prism-cli": "^5.14.2",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/addon-interactions": "^8.6.12",
    "@storybook/addon-links": "^8.6.12",
    "@testing-library/vue": "^8.1.0",
    "@types/node": "^22.15.3",
    "@vue/test-utils": "^2.4.6",
    "cross-env": "^7.0.3",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "globals": "^16.0.0",
    "happy-dom": "^17.4.4",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "npm-run-all2": "^7.0.2",
    "postcss-html": "^1.8.0",
    "prettier": "^3.5.3",
    "storybook": "^8.6.12",
    "stylelint": "^16.19.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recess-order": "^6.0.0",
    "stylelint-config-recommended-vue": "^1.6.0",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-prettier": "^5.0.3",
    "unplugin-auto-import": "^19.1.2",
    "unplugin-vue-components": "^28.5.0",
    "vitest": "^3.1.2",
    "vue-tsc": "^2.2.10"
  }
}
